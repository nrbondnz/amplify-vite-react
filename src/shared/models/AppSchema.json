{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "IDataState",
  "type": "object",
  "properties": {
    "machines": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IMachine"
      }
    },
    "exercises": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IExercise"
      }
    },
    "users": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IUser"
      }
    },
    "workouts": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IWorkout"
      }
    },
    "userWorkoutExercises": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IUserWorkoutExercise"
      }
    },
    "actionStates": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IActionState"
      }
    },
    "userExercises": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IUserExercise"
      }
    },
    "locations": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/ILocation"
      }
    },
    "muscles": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/IMuscle"
      }
    },
    "loading": {
      "type": "boolean"
    }
  },
  "required": [
    "machines",
    "exercises",
    "users",
    "workouts",
    "userWorkoutExercises",
    "actionStates",
    "userExercises",
    "locations",
    "muscles",
    "loading"
  ],
  "definitions": {
    "IMachine": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "name": { "type": "string" },
        "numOfMachine": { "type": "string" },
        "idLocation": { "type": "string" },
        "settings": {
          "type": "object",
          "additionalProperties": { "type": "string" }
        }
      },
      "required": ["id", "name", "numOfMachine", "idLocation", "settings"]
    },
    "IExercise": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "name": { "type": "string" },
        "idMachine": { "type": "string" },
        "description": { "type": "string" }
      },
      "required": ["id", "name", "idMachine", "description"]
    },
    "IUser": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "name": { "type": "string" },
        "email": { "type": "string" },
        "phoneNumber": { "type": "string" },
        "idLocation": { "type": "string" },
        "roles": {
          "type": "array",
          "items": { "type": "string" }
        }
      },
      "required": ["id", "name", "email", "phoneNumber", "idLocation", "roles"]
    },
    "IWorkout": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "idUser": { "type": "string" },
        "name": { "type": "string" }
      },
      "required": ["id", "idUser", "name"]
    },
    "IUserWorkoutExercise": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "name": { "type": "string" },
        "idUser": { "type": "string" },
        "idWorkout": { "type": "string" },
        "idMachine": { "type": "string" },
        "idExercise": { "type": "string" },
        "max": { "type": "string" },
        "settings": {
          "type": "object",
          "additionalProperties": { "type": "string" }
        }
      },
      "required": ["id", "name", "idUser", "idWorkout", "idMachine", "idExercise", "max", "settings"]
    },
    "IActionState": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "LOADED": { "type": "string" },
        "appPage": { "type": "string" },
        "appCommand": { "type": "string" },
        "primId": { "type": "string" },
        "primObj": { "type": "object" },
        "primName": { "type": "string" },
        "secId": { "type": "string" },
        "secObj": { "type": "object" },
        "secName": { "type": "string" }
      },
      "required": ["id", "LOADED"]
    },
    "IUserExercise": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "idUser": { "type": "string" },
        "idExercise": { "type": "string" }
      },
      "required": ["id", "idUser", "idExercise"]
    },
    "IMuscle": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "name": { "type": "string" },
        "numOfMuscle": { "type": "string" },
        "description": { "type": "string" },
        "parent": {
          "type": "string",
          "nullable": true
        }
      },
      "required": ["id", "name", "numOfMuscle", "description"]
    },
    "ILocation": {
      "type": "object",
      "properties": {
        "id": { "type": "number" },
        "name": { "type": "string" }
      },
      "required": ["id", "name"]
    }
  }
}